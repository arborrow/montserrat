{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/psr7": "^1.3",
        "intervention/image": "^2.4",
        "laravel/framework": "^6.6",
        "laravel/helpers": "^1.1",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "6.0.*",
        "mailgun/mailgun-php": "^2.1",
        "php-http/curl-client": "^1.7",
        "spatie/laravel-google-calendar": "^2.3",
        "stuyam/laravel-phone-validator": "^1.0",
        "vinkla/hashids": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "jasonmccreary/laravel-test-assertions": "^0.1.0",
        "laravel/homestead": "^7.17",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-test-factory-helper": "^1.2",
        "nunomaduro/collision": "^3.0",
        "orangehill/iseed": "^2.6.2",
        "phpunit/phpunit": "^8.0",
        "symfony/css-selector": "~3.1",
        "symfony/dom-crawler": "~3.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "homestead make"
        ]
    }
}
